{
  "name": "gplanchat/sklodowska",
  "type": "library",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10 || ^11",
    "phpstan/phpstan": "^1.10"
  },
  "require": {
    "modelflow-ai/api-client": "dev-",
    "modelflow-ai/core": "@dev",
    "modelflow-ai/mistral": "@dev",
    "modelflow-ai/ollama": "@dev",
    "modelflow-ai/ollama-adapter": "@dev",
    "modelflow-ai/mistral-adapter": "@dev",
    "modelflow-ai/openai-adapter": "@dev",
    "symfony/dotenv": "^6 || ^7"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
